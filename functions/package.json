  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/KentDevelopment/lead-app-functions.git"
  },
  "scripts": {
    "clean": "rm -rf package-lock.json firestore-debug.log ui-debug.log node_modules",
    "clean:install": "npm run clean && npm i",
    "deploy:prod": "npm run lint --fix && firebase deploy  -P production --only functions",
    "deploy": "npm run lint --fix && firebase deploy -P development --only functions",
    "fix": "eslint . --fix",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint": "eslint .",
    "logs": "firebase functions:log",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.14.1",
    "nodemailer": "^6.6.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2"
  }
}
