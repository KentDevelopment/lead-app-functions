{
  "name": "lead-app-email-functions",
  "version": "1.2.2",
  "description": "User Registration Email",
  "homepage": "https://lead.kent.edu.au/",
  "bugs": {
    "url": "https://github.com/KentDevelopment/lead-app-functions/issues",
